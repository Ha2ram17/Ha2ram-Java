package jdbc;

public class pc {
	String 이름;
	int 공격력;
	int 체력;
	int 마나;
	
	public String get이름() {
		return 이름;
	}
	public void set이름(String 이름) {
		this.이름 = 이름;
	}
	public int get공격력() {
		return 공격력;
	}
	public void set공격력(int 공격력) {
		this.공격력 = 공격력;
	}
	public int get체력() {
		return 체력;
	}
	public void set체력(int 체력) {
		this.체력 = 체력;
	}
	public int get마나() {
		return 마나;
	}
	public void set마나(int 마나) {
		this.마나 = 마나;
	}
	//메소드(행동)
	void 공격(pc g) {
		System.out.printf("%s가 %s를 공격합니다.\n",이름,g.이름);
		g.체력 -= 공격력;
		System.out.printf("%s의 체력이 %d가 되었습니다.\n",h.이름,h.체력);
	}
	
}
